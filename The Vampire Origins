local AttackSection = Tab:NewSection("Server-Attacks ( Creds to nut )")

AttackSection:NewButton("Siphon all", "Be a siphoner to siphon the server", function()
    print("Clicked")
    local Plr = game.Players.LocalPlayer
    for i, Player in pairs (game.Players:GetPlayers()) do

        if Player.Character ~= nil and Player ~= Plr then
            local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
            if mag < 100000 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
                
local args = {
[1] = Target,
[2] = "Siphon",
[3] = "Target"
}

game:GetService("ReplicatedStorage").Events.Server.SpellEventHandler:FireServer(unpack(args))


            end
        end
end   
end)

AttackSection:NewButton("Push all", "ButtonInfo", function()
    print("Clicked")
    local Plr = game.Players.LocalPlayer
    for i, Player in pairs (game.Players:GetPlayers()) do

        if Player.Character ~= nil and Player ~= Plr then
            local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
            if mag < 100000 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
                
local args = {
[1] = Target,
[2] = "TelekineticPush1",
[3] = Vector3.new(-0.6924017071723938, -0.3220159113407135, -0.6456667184829712)
}

game:GetService("ReplicatedStorage").Events.Server.SpellEventHandler:FireServer(unpack(args))


            end
        end
end   
end)
